import java.util.*;

public class Main {

    public static ArrayList<Integer> solve(ArrayList<Integer> a1 , ArrayList<Integer> a2) {
        a1.addAll(a2);
        Set<Integer> set = new TreeSet<Integer>(a1);
        return new ArrayList<>(set);
    }

    public static void main(String[] args) {
        ArrayList<Integer> a1 = new ArrayList<Integer>(Arrays.asList(1 , 4 , 6 , 9 , 11 , 2 , 7 , 3 , 8 )) ;
        ArrayList<Integer> a2 = new ArrayList<Integer>(Arrays.asList(1 , 3 , 2 , 4 , 5 , 10 , 11 , 6)) ;

        System.out.println(solve(a1, a2));

    }
}
